<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.MainMapper">
	<select id="gongji" resultType="com.example.demo.vo.GongjiVo">
		select * from gongji order by no desc limit 5
	</select>
	
   <select id="getChong" resultType="int">
      select ceil( count(*)/10 ) from gongji
   </select>
	
</mapper>